    if(k<5)
        i = 1;
    else if(k<10)
        i = 2;
    else
        i = 3;

    k = k - 1;
}


for i = 1:N {
    if(i<=N/4)
        print i;
    else if(i<=N/2)
        break;
    else
        continue;
}

