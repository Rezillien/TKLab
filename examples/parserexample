# control flow instruction

N = 10;
M = 20;
for i = 1:N {
    for j = i:M {
        print i, j;
    }
}

if(N==10)
    print "N==10";
else if(N!=10)
    print "N!=10";

if(N>5)
    print "N>5";
else if(N>=0)
    print "N>=0";

if(N<10)
    print "N<10";
else if(N<=15)
    print "N<=15";

